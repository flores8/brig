<h1>Listing private_memories</h1>

<table>
  <thead>
    <tr>
      <th>Body</th>
      <th>Name</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @private_memories.each do |private_memory| %>
      <tr>
        <td><%= private_memory.body %></td>
        <td><%= private_memory.name %></td>
        <td><%= link_to 'Show', private_memory %></td>
        <td><%= link_to 'Edit', edit_private_memory_path(private_memory) %></td>
        <td><%= link_to 'Destroy', private_memory, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Private memory', new_private_memory_path %>
